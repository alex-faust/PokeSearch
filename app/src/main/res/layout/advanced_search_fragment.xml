    <data>
        <variable
            name="advancedViewModel"
            type="com.example.pokesearch.search.AdvancedSearchViewModel" />
    </data>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
